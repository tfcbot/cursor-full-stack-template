{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "TEST_TYPE=unit vitest run tests/unit/",
    "test:integration": "TEST_TYPE=integration vitest run tests/integration/",
    "test:all": "vitest run",
    "test:api": "vitest run tests/unit/api-service.test.ts tests/integration/api-service.test.ts --coverage",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.71.1",
    "dotenv": "^16.4.7",
    "next": "15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "cross-fetch": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "is-fullwidth-code-point-cjs": "npm:is-fullwidth-code-point@^3.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "string-width-cjs": "npm:string-width@^4.2.0",
    "strip-ansi-cjs": "npm:strip-ansi@^6.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.1.1"
  }
}
